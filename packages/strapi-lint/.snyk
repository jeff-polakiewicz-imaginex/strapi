# Snyk (https://snyk.io) policy file, patches or ignores known vulnerabilities.
version: v1.13.5
# ignores vulnerabilities until expiry date; change duration by modifying expiry date
ignore:
  shelljs:
    - '*':
        reason: testing
        expires: 2019-01-04T14:37:04.113Z
# patches apply the minimum changes required to fix a vulnerability
patch:
  SNYK-JS-LODASH-450202:
    - babel-eslint > @babel/traverse > lodash:
        patched: '2019-07-04T00:01:55.300Z'
    - babel-eslint > @babel/traverse > @babel/generator > lodash:
        patched: '2019-07-04T00:01:55.300Z'
    - babel-eslint > @babel/traverse > @babel/helper-function-name > @babel/template > lodash:
        patched: '2019-07-04T00:01:55.300Z'
    - babel-eslint > @babel/traverse > @babel/helper-function-name > @babel/template > @babel/types > lodash:
        patched: '2019-07-04T00:01:55.300Z'
